workspace 'Gureum'
use_frameworks!

target 'OSXCore' do
    platform :osx, "10.13"
    project 'Gureum.xcodeproj'
    pod 'cdebug'
    pod 'FoundationExtension', '~> 2.0.0'
end
target 'OSX' do
    platform :osx, "10.13"
    project 'Gureum.xcodeproj'
    pod 'FoundationExtension', '~> 2.0.0'
    pod 'Fabric'
    pod 'Crashlytics'
end
target 'Preferences' do
    platform :osx, "10.13"
    project 'Gureum.xcodeproj'
    pod 'MASShortcut'
end
target 'OSXTestApp' do
    platform :osx, "10.13"
    project 'Gureum.xcodeproj'
    pod 'MASShortcut'
    pod 'FoundationExtension', '~> 2.0.0'
end
target 'OSXTests' do
    platform :osx, "10.13"
    project 'Gureum.xcodeproj'
end

